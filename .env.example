# Push Notifications (optional)
EXPO_PUSH_TOKEN=your_expo_push_token_here

# Social Auth (optional)
GOOGLE_CLIENT_ID=your_google_client_id_here
APPLE_CLIENT_ID=your_apple_client_id_here
